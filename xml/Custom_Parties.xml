<?xml version="1.0" encoding="UTF-8"?>
<window id="1804">
	<!-- skinsettings -->
	<defaultcontrol always="true">9000</defaultcontrol>
	<controls>
		<!-- Background -->
		<include>WindowBackgroundImage</include>
		<control type="group">
			<animation type="WindowOpen">
				<effect type="fade" start="0" end="100" time="300" tween="cubic" easing="inout"/>
				<effect type="zoom" start="120" end="100" time="400" tween="cubic" easing="out" center="auto"/>
			</animation>
			<animation type="WindowClose">
				<effect type="fade" start="100" end="0" time="300" tween="cubic" easing="inout"/>
				<effect type="zoom" start="100" end="120" time="400" tween="cubic" easing="out"center="auto"/>
			</animation>
			<include content="Time">
				<param name="heading">Parties</param>
			</include>
			<control type="label">
				<animation type="Visible">
					<effect type="fade" start="0" end="100" time="300" tween="cubic" easing="inout" center="auto"/>
				</animation>
				<animation type="Hidden">
					<effect type="fade" start="100" end="0" time="300" tween="cubic" easing="inout" center="auto"/>
				</animation>
				<left>357</left>
				<top>150</top>
				<width>900</width>
				<height>233</height>
				<label>Parties You Are Invited To And Friends' Parties</label>
				<font>Font36</font>
				<textcolor>white</textcolor>
				<align>left</align>
				<autoscroll>false</autoscroll>
			</control>
			<control type="panel" id="9000">
				<animation type="Conditional" condition="true">
					<effect type="zoom" start="100" end="85" time="1" tween="cubic" easing="inout" center="auto"/>
				</animation>
				<left>220</left>
				<top>250</top>
				<width>1895</width>
				<height>825</height>
				<focusposition>1</focusposition>
				<defaultcontrol>1</defaultcontrol>
				<onup>-</onup>
				<ondown>-</ondown>
				<onleft>-</onleft>
				<onright>-</onright>
				<orientation>vertical</orientation>
				<scrolltime tween="sine" easing="out">240</scrolltime>
				<itemlayout width="1290" height="180">
					<control type="group">
						<control type="image">
							<left>0</left>
							<top>170</top>
							<width>1340</width>
							<height>2</height>
							<texture>color_white.png</texture>
							<colordiffuse>80FFFFFF</colordiffuse>
						</control>
						<control type="image">
							<left>0</left>
							<top>10</top>
							<width>160</width>
							<height>160</height>
							<texture fallback="icons/xmb/weather.png">$INFO[ListItem.Thumb]</texture>
						</control>
						<control type="label">
							<left>200</left>
							<top>-40</top>
							<width>900</width>
							<height>233</height>
							<label>$INFO[ListItem.Label]</label>
							<font>Font36</font>
							<textcolor>white</textcolor>
