							<width>900</width>
							<height>233</height>
							<label>$INFO[ListItem.Label2]</label>
							<font>Font27</font>
							<textcolor>white</textcolor>
							<align>left</align>
							<autoscroll>false</autoscroll>
						</control>
					</control>
				</itemlayout>
				<focusedlayout width="1290" height="180">
